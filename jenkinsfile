pipeline {
    agent any

    tools {
        maven "3.6.3"
    }

    stages {
        stage("Build") {
            steps {
                sh "mvn -version"
                sh "mvn clean install"
            }
        }
        stage("Deploy ap to vm"){
             steps{
                  sh "ansiblePlaybook -i ansible/inventory-file ansible/playbook.yml --private-key=/Users/caglar/.ssh/caglar_test_key.pem"
             }
        }
    }
}